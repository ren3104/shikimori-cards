{
    "github": {
        "silent": true
    },
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "env": {
        "VERCEL_CLI_VERSION": "vercel@31.0.1"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}
